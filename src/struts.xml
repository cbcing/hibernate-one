<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="default" namespace="/" extends="struts-default">
        <action name="queryCustomers" class="com.scujcc.david.action.CustomerAction" method="execute">
            <result type="dispatcher" name="success">customers.jsp</result>
        </action>
        <action name="queryOrders" class="com.scujcc.david.action.OrderAction" method="execute">
            <result type="dispatcher" name="success">orders.jsp</result>
        </action>
    </package>
</struts>